{
    "name": "5balloons/laravel-ad-manager",
    "description": "Ad, Banners, Callouts Manager for Laravel",
    "keywords": [
        "5balloons",
        "laravel-ad-manager"
    ],
    "homepage": "https://github.com/5balloons/laravel-ad-manager",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tushar Gugnani",
            "email": "tushar@5balloons.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "_5balloons\\LaravelAdManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_5balloons\\LaravelAdManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "_5balloons\\LaravelAdManager\\LaravelAdManagerServiceProvider"
            ],
            "aliases": {
                "LaravelAdManager": "_5balloons\\LaravelAdManager\\LaravelAdManagerFacade"
            }
        }
    }
}
